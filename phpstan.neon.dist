includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app
        - bootstrap
        - config
        - database
        - routes

    excludePaths:
        analyse:
            - database/factories/*
            - tests/*

    tmpDir: var/phpstan

    level: 5

    checkOctaneCompatibility: true

    ignoreErrors: []
